@{
    switch(UrlData[0].ToString().ToLowerInvariant()) {
        case "windowslive":
            OAuthWebSecurity.RequestAuthentication("windowsLive", "~/Account/Land/WindowsLive");
            break;
        case "facebook":
            OAuthWebSecurity.RequestAuthentication("facebook", "~/Account/Land/Facebook");
            break;
    }

    Response.StatusCode = 404;
}